{
  "name": "react-test-template",
  "version": "1.0.0",
  "description": "Template repository for React project with testing via Jest and Enzyme",
  "repository": "https://github.com/siwatpru/react-test-template",
  "main": "index.js",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "scripts": {
    "build": "webpack -d",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch"
  },
  "author": "Siwat Pruksapanya <siwat.pru@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "enzyme-to-json": "^1.5.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^2.8.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "nock": "^9.0.13",
    "react-addons-test-utils": "^15.4.2",
    "redux-mock-store": "^1.2.3",
    "sinon": "^2.1.0"
  }
}
